body {
  font-family:'Raleway',Helvetica,Arial,sans-serif;
  font-weight:400;background-color:rgba(0,0,0,0.02);
}

button {
  font-family:'Raleway',Helvetica,Arial,sans-serif;
  width: 200px;
  border-radius: 10px;
  background: #61b8db;
  background-image: -webkit-linear-gradient(top, rgba(97,184,219,0), rgba(48,141,196,1));
  padding: 20px 20px 20px 20px;
  font-size: 125%;
  margin-left: 0px;
}

.about {
  padding: 50px 0 200px 0;
}

.about h1, .about p {
  color: white;
  font: serif;
}

.about-wrapper {
  background: url('<%= asset_path("moose3.png") %>') no-repeat;
  background-size: cover;
  border-bottom: solid #cecece 1px;
}

.about img{
  width:250px;
  height:250px;
  border-radius:150px;
  -webkit-border-radius:150px;
  -moz-border-radius:150px;
}

.skills-section {
  padding: 50px 0 50px 0;
}

.skills-section img {
  width: 240px;
  height: 192px;
  margin-top: 15px;
  margin-left: auto;
  margin-right: auto;
  display:block;
  border: white;
  margin-bottom: 5px;
}

.skills-section div {
  text-align: center;
  border-radius: 10px;
}

.skills-section .large-4 {
  background-color: white; 
}

.skills-section .small-12 {
  background-color: white;
}

.skills-section h3 {
  margin: 30px 0 30px 0;
}

button.projects {
  margin: 30px 0 0 0;
}

.skills-wrapper {
  background: url('<%= asset_path("light_grey.png") %>');
  border-bottom: solid #cecece 1px;
}

.projects-wrapper {
  background: url('<%= asset_path("crossword.png") %>');
}

.projects-section img {
  width: 970px;
  height: 469px;
}

.alignleft {
  float: left;
}

.alignright {
  float: right;
}

.contact-wrapper {
  background-color: #303030;
}

.contactbox h3 {
  color: white;
  padding: 20px 0 0px 0;
}

.contact-section img {
  height: 60px;
  width: 60px;
  margin: 0 10px 0 10px;
  -webkit-transition: all 0.5s ease;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
}

.contact-section img:hover {
  margin-top: -20px;
  -webkit-transition: all 0.5s ease;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
}

.inline-list {
  margin-top: 20px;
  padding-top: 20px;
}

hr {
  border:0;
  height:1px;
  background-image:-webkit-linear-gradient(left,rgba(0,0,0,0),rgba(0,0,0,0.75),rgba(0,0,0,0));
  background-image:-moz-linear-gradient(left,rgba(0,0,0,0),rgba(0,0,0,0.75),rgba(0,0,0,0));
  background-image:-ms-linear-gradient(left,rgba(0,0,0,0),rgba(0,0,0,0.75),rgba(0,0,0,0));
  background-image:-o-linear-gradient(left,rgba(0,0,0,0),rgba(0,0,0,0.75),rgba(0,0,0,0));
  opacity:0.8;
}
footer {
  padding:0px 0 10px 0;
  color: white;
  background-color:#303030;
}



















